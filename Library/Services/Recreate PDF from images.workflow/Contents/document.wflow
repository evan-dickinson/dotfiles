					<key>COMMAND_STRING</key>
					<string># This step converts images to PDFs. There's an automator task to do
# the same thing, but you can't use variables to control its output.

pdf_filenames = []
ARGV.each do |png_filename|
	pdf_filename = png_filename.gsub(/ \.png $ /x, '.pdf')
	pdf_filenames &lt;&lt; pdf_filename
	
	# cupsfilter converts files to PDF. It prints its output
	# to stdout, which we then capture and put into a file.
	pdf_data = %x[/usr/sbin/cupsfilter #{png_filename}]
    File.open(pdf_filename, "w") do |file|
	  file.write(pdf_data)
    end
end

puts pdf_filenames.join("\n")
